---
const { props } = Astro
const { src, ...options } = props

---

<script>
	import { register } from 'shared/main'
	import * as iframe from './index'
	register('app-iframe', iframe)
</script>

<app-iframe>
	<iframe data-src={src} {...options}></iframe>
</app-iframe>

<style>
	x-iframe {
		display:block
	}
</style>